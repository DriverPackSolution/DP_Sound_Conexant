;;HKLM, "Software\Dolby\PCEE4\StaticKeys",   "{DD0DF94B-53A2-4a92-854E-91F42E28FD7B},51",   %REG_DWORD%, 0x00000000; PCEE4_ALLOW_AO_IN_DEMO_BYPASS_PID
;;HKLM, "Software\Dolby\PCEE4\StaticKeys",   "{DD0DF94B-53A2-4a92-854E-91F42E28FD7B},52",   %REG_DWORD%, 0x00000000; PCEE4_ALLOW_AR_IN_BYPASS_PID
